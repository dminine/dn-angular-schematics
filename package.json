{
  "name": "dn-angular-schematics",
  "version": "0.0.11",
  "scripts": {
    "build:my-lib": "ng build dn-schematics && cd projects/dn-schematics && npm run build && npm run postbuild && cd ../../ && npm run copy:thirdPart",
    "link": "npm link ./dist/dn-schematics",
    "unlink": "npm unlink dist/dn-schematics",
    "generate:schematic:entity": "ng generate dn-schematics:entity-service --name my-data",
    "generate:schematic:adminList": "ng generate dn-schematics:admin-list --name list-page",
    "npm:publish:public": "cd dist/dn-schematics && npm publish --access public",
    "copy:thirdPart": "cp -a projects/dn-schematics/schematics/third_party dist/dn-schematics/schematics"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.5",
    "@angular/common": "^15.2.5",
    "@angular/compiler": "^15.2.5",
    "@angular/core": "^15.2.5",
    "@angular/forms": "^15.2.5",
    "@angular/platform-browser": "^15.2.5",
    "@angular/platform-browser-dynamic": "^15.2.5",
    "@angular/router": "^15.2.5",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^15.0.0",
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular/cli": "^15.2.4",
    "@angular/compiler-cli": "^15.2.5",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^14.18.42",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^15.2.2",
    "typescript": "~4.8.4"
  }
}
